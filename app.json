{
  "expo": {
    "name": "RailEase",
    "slug": "railease",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/app_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-bg.png",
      "resizeMode": "native",
      "backgroundColor": "#1E40AF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raillovertej.boltexponativewind",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logo/app_logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-splash-screen",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "53697233-ad1f-450b-8779-a15a41d5e4b1"
      }
    },
    "owner": "raillovertej",
    "android": {
      "package": "com.raillovertej.boltexponativewind"
    }
  }
}
